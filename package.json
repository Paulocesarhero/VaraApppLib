{
  "name": "varaapplib",
  "version": "1.0.9",
  "description": "Aquí se guardan todos los componentes de react native que se compartiran entre proyectos estos al mismo tiempo seran subido a un repositorio de npm",
  "main": "node_modules/expo/AppEntry.js",
  "files": [
    "components/**/*",
    "!**/storybook/**/*",
    ".npmignore"
  ],
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-generate": "sb-rn-get-stories --config-path .ondevice",
    "storybook": "cross-env STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "cross-env STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "cross-env STORYBOOK_ENABLED='true' expo start --android"
